<h1>Login</h1>
<div class="col">
  <%= form_for(@user, url: login_path) do |f| %>
    <div class="mb-3">
      <label for="" class="form-label">
        <%= f.label "Email" %>
      </label><br>
      <%= f.email_field :email %>
    </div>

    <div class="mb-3">
      <label for="" class="form-label">
        <%= f.label "Password" %>
      </label><br>
      <%= f.password_field :password %>
    </div>
    <%= f.submit "Login" %>
  <% end %>
